<Page x:Class="Katkov362.Pages.TeacherPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Katkov362.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Background="#535754"
      Title="TeacherPage">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Анкеты" Name="QuestionnaireButton" Click="QuestionnaireButton_Click"></Button>
            <Button Content="Выдача заданий" Name="GiveButton" Click="GiveButton_Click"></Button>
            <Button Content="Добавление учеников" Name="AddStudentButton" Click="AddStudentButton_Click"></Button>
            
        </StackPanel>
    </Grid>
</Page>
